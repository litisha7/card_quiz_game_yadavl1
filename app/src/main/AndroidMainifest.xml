<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.card_quiz_game_yadavl1">

<!--    All App Activities are defined here-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/quiz_logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/quiz_logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Card_quiz_game_tilloop1">
        <activity android:name="com.example.card_quiz_game_yadavl1.ShowResult"></activity>
        <activity android:name="com.example.card_quiz_game_yadavl1.ScoreStats"></activity>
        <activity android:name="com.example.card_quiz_game_yadavl1.Quiz" />
        <activity android:name="com.example.card_quiz_game_yadavl1.GameRules" />
        <activity android:name="com.example.card_quiz_game_yadavl1.Login" />
        <activity android:name="com.example.card_quiz_game_yadavl1.Register" />
<!--        The launcher activity-->
        <activity android:name="com.example.card_quiz_game_yadavl1.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.card_quiz_game_yadavl1.MainActivity" />
    </application>

</manifest>
